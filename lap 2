import pandas as pd

data1 = {
    "thing": ["Widget A", "Widget B", "Widget C"],
    "num": ["$1,234.50", "$567.89", "$2,345.00"],
    "howmany": [10, 5, None],
}

df1 = pd.DataFrame(data1)

df1["num"] = df1["num"].apply(
    lambda x: float(x.replace("$", "").replace(",", ""))
)

df1["how many"] = df1["how many"].fillna(0)

df1["all"] = df1["num"] * df1["how many"]

df1["lvl"] = df1["num"].apply(
    lambda x: "low" if x < 1000 else "med" if x < 2000 else "high"
)

print(df1) 
